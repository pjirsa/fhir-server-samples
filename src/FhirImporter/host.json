{
    "version": "2.0",
    "extensions": {
        "queues": {
            "maxPollingInterval": "00:00:02",
            "visibilityTimeout": "00:00:30",
            "batchSize": 8,
            "maxDequeueCount": 10,
            "newBatchThreshold": 4
        },
        "durableTask": {
            "maxConcurrentActivityFunctions": 4,
            "maxConcurrentOrchestratorFunctions": 1,
            "hubName": "FhirTelemetryHub"
        }
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": false
            }
        }
    }
}
